USE tncl225;
create tables;
SHOW TABLES;
insert functions;
USE tablename (addresses, receipts);
SHOW COLUMNS * FROM tablename;
SELECT * FROM tablename;


SELECT formalName AS "Customer/Business Full Name", CONCAT(address1, IFNULL(CONCAT(' ', address2), ''), ' ', city, ', ', stateABB, ' ', zipcode) AS "Delivery address" FROM addresses;

SELECT COUNT(*) AS "Total number" FROM addresses WHERE formalName LIKE '%Bo%';
SELECT COUNT(*) AS "Total number" FROM receipts WHERE formalName LIKE '%Bo%';

ALTER TABLE addresses
ADD primary_phone int(10);
ALTER TABLE addresses
ADD secondary_phone int(10);

