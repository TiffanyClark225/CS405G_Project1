CREATE TABLE addresses (
    ID int(2),
    firstName char(40),
    lastName char(40),
    address1 char(40),
    address2 char(40),
    city char(40),
    stateABB char(2),
    zipcode int(5),
);

CREATE TABLE receipts (
    formalName char(40),
    date_of_purchase date,
    totalSales DECIMAL(5,2),
    num_of_items int(3),
    picture blob(50)
);

